{
  "name": "myhospital",
  "version": "0.0.0",
  "private": true,
  "description": "医保定点医院选择助手",
  "scripts": {
    "postinstall": "umi g tmp",
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint:prettier": "prettier --check \"**/*\" --end-of-line auto",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx --format=pretty ./src",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\"",
    "start": "umi dev",
    "dev": "npm run start:dev",
    "start:dev": "cross-env PORT=9000 REACT_APP_ENV=dev MOCK=none umi dev",
    "start:no-mock": "cross-env PORT=9000 MOCK=none umi dev",
    "start:no-ui": "cross-env PORT=9000 UMI_UI=none umi dev",
    "start:pre": "cross-env PORT=9000 REACT_APP_ENV=pre umi dev",
    "start:test": "cross-env PORT=9000 REACT_APP_ENV=test MOCK=none umi dev"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.0.5",
    "@ant-design/pro-layout": "^5.0.8",
    "antd": "4.0.4",
    "classnames": "^2.2.6",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "omit.js": "^1.0.2",
    "path-to-regexp": "^6.1.0",
    "prop-types": "^15.7.2",
    "qs": "^6.9.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet-async": "^1.0.4",
    "sockjs-client": "^1.4.0",
    "stompjs": "^2.3.3",
    "umi": "^3.0.16",
    "umi-request": "^1.2.19",
    "use-merge-value": "^1.0.1"
  },
  "devDependencies": {
    "@umijs/fabric": "^2.0.7",
    "@umijs/preset-ant-design-pro": "^1.0.1",
    "@umijs/preset-react": "^1.4.8",
    "@umijs/preset-ui": "^2.1.7",
    "chalk": "^4.0.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.1.1",
    "mockjs": "^1.1.0",
    "prettier": "^2.0.1",
    "stylelint": "^13.2.1"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ]
}
